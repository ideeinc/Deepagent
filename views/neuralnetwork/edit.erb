<!DOCTYPE html>
<%#include "neuralnetwork.h" %>
<% tfetch(QVariantMap, neuralNetwork); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Editing Neural Network</h1>

<%== formTag(urla("save", neuralNetwork["id"]), Tf::Post) %>
  <p>
    <label>ID<br /><input type="text" name="neuralNetwork[id]" value="<%= neuralNetwork["id"] %>" readonly="readonly" /></label>
  </p>
  <p>
    <label>Layers<br /><input type="text" name="neuralNetwork[layers]" value="<%= neuralNetwork["layers"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Update" />
  </p>
</form>

<%== linkTo("Show", urla("show", neuralNetwork["id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>
